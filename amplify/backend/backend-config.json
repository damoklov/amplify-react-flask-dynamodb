{
  "storage": {
    "gamestoredb": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "iot": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "gamestoreapi": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "gamestoredb",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    }
  },
  "api": {
    "gamestoreapi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "gamestoreapi",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "iot": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "gamestoreapi",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}